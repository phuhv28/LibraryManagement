<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Rectangle fill="#474242" height="450.0" width="600.0" />
        <ImageView fitHeight="50.0" fitWidth="50.0" layoutX="1.0" layoutY="2.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/image_home.png" />
            </image>
        </ImageView>
        <Button fx:id="btHome" layoutX="7.0" layoutY="5.0" mnemonicParsing="false" opacity="0.0" prefHeight="43.0" prefWidth="38.0" text=" " textFill="#474242" />
        <ImageView fitHeight="52.0" fitWidth="60.0" layoutX="537.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/image_setting.png" />
            </image>
        </ImageView>
        <Button fx:id="btSetting" layoutX="547.0" layoutY="7.0" mnemonicParsing="false" opacity="0.0" prefHeight="40.0" prefWidth="40.0" text=" " textFill="#474242" />
        <ImageView fitHeight="50.0" fitWidth="50.0" layoutX="487.0" layoutY="4.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/image_account.png" />
            </image>
        </ImageView>
        <Button fx:id="btUser" layoutX="487.0" layoutY="5.0" mnemonicParsing="false" opacity="0.0" prefHeight="44.0" prefWidth="50.0" text=" " textFill="#474242" />
        <Label layoutX="193.0" layoutY="87.0" style="-fx-font-weight: BOLD;" text="FIX DOCUMENT" textFill="#f2f2f2">
            <font>
                <Font size="24.0" />
            </font></Label>
        <Label layoutX="106.0" layoutY="144.0" prefHeight="68.0" prefWidth="165.0" style="-fx-font-weight: bold;" text="Enter the ID of the document to edit :" textFill="WHITE" wrapText="true">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <MenuButton fx:id="mbFilter" layoutX="310.0" layoutY="208.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="166.0" text="Fitler">
            <items>
                <MenuItem mnemonicParsing="false" text="Title" />
                <MenuItem mnemonicParsing="false" text="Author" />
                <MenuItem mnemonicParsing="false" text="Publisher" />
                <MenuItem mnemonicParsing="false" text="Publication Year" />
                <MenuItem mnemonicParsing="false" text="Total Quantity" />
            </items>
        </MenuButton>
        <TextField fx:id="tfDocumentID" layoutX="309.0" layoutY="158.0" prefHeight="35.0" prefWidth="166.0" promptText="Document ID" />
        <TextField fx:id="tfInformation" layoutX="310.0" layoutY="256.0" prefHeight="35.0" prefWidth="166.0" promptText="Information" />
        <Label layoutX="107.0" layoutY="219.0" style="-fx-font-weight: bold;" text="Select criteria to edit :" textFill="#fafafa">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Label layoutX="107.0" layoutY="269.0" style="-fx-font-weight: bold;" text="Enter Info with Filters :" textFill="#f7f7f7">
            <font>
                <Font size="18.0" />
            </font></Label>
        <Button fx:id="btFixDocument" layoutX="309.0" layoutY="311.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="70.0" style="-fx-background-radius: 15; -fx-font-weight: bold;" text="FIX">
            <font>
                <Font size="18.0" />
            </font></Button>
    </children>

</AnchorPane>
