<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Rectangle fill="#474242" height="450.0" width="600.0" />
        <ImageView fitHeight="50.0" fitWidth="50.0" layoutX="1.0" layoutY="2.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/image_home.png" />
            </image>
        </ImageView>
        <Button fx:id="btHome" layoutX="7.0" layoutY="5.0" mnemonicParsing="false" opacity="0.0" prefHeight="43.0" prefWidth="38.0" text=" " textFill="#474242" />
        <ImageView fitHeight="52.0" fitWidth="60.0" layoutX="537.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/image_setting.png" />
            </image>
        </ImageView>
        <Button fx:id="btSetting" layoutX="547.0" layoutY="7.0" mnemonicParsing="false" opacity="0.0" prefHeight="40.0" prefWidth="40.0" text=" " textFill="#474242" />
        <ImageView fitHeight="50.0" fitWidth="50.0" layoutX="487.0" layoutY="4.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/image_account.png" />
            </image>
        </ImageView>
        <Button fx:id="btUser" layoutX="487.0" layoutY="5.0" mnemonicParsing="false" opacity="0.0" prefHeight="44.0" prefWidth="50.0" text=" " textFill="#474242" />
        <Label layoutX="193.0" layoutY="87.0" style="-fx-font-weight: BOLD;" text="SEARCH DOCUMENT" textFill="#f2f2f2">
            <font>
                <Font size="24.0" />
            </font></Label>
        <MenuButton fx:id="mbFilter" layoutX="336.0" layoutY="144.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="150.0" text="Filter">
            <items>
                <MenuItem fx:id="miISBN" mnemonicParsing="false" text="ISBN" />
                <MenuItem fx:id="miName" mnemonicParsing="false" text="Name" />
                <MenuItem fx:id="miAutthor" mnemonicParsing="false" text="Author" />
                <MenuItem mnemonicParsing="false" text="Category" />
            </items>
        </MenuButton>
        <Label layoutX="126.0" layoutY="144.0" style="-fx-font-weight: BOLD;" text="Search Criteria :" textFill="WHITE">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Label layoutX="128.0" layoutY="201.0" style="-fx-font-weight: bold;" text="Enter Info with Filters :" textFill="WHITE">
            <font>
                <Font size="18.0" />
            </font></Label>
        <TextField fx:id="tfInformation" layoutX="339.0" layoutY="198.0" />
        <Button fx:id="btSearchDocument" layoutX="343.0" layoutY="250.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="84.0" style="-fx-font-weight: bold; -fx-background-radius: 15;" text="Search">
            <font>
                <Font size="16.0" />
            </font>
        </Button>
    </children>

</AnchorPane>
